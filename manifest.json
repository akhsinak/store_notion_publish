{
  "manifest_version": 3,
  "name": "StoreNotion",
  "version": "0.1.0",
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+L",
        "mac": "MacCtrl+Shift+N",
        "chromeos": "Ctrl+Shift+L",
        "linux": "Ctrl+Shift+L"
      }
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "notiord"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "host_permissions": [
    "https://*/*"
  ],
  "icons": {
    "16": "s16.png",
    "48": "s48.png",
    "128": "s128.png"
  }
}